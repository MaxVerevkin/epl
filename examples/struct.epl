struct Vec2 {
	x: i32,
	y: i32,
}

fn vec2_add(lhs: Vec2, rhs: Vec2) -> Vec2 {
	lhs.x += rhs.x;
	lhs.y += rhs.y;
	lhs
}

fn vec2_print(vec: Vec2) {
	printf("Vec2 .{ x: %i, y: %i }\n", vec.x, vec.y);
}

fn main() -> i32 {
	let vec = Vec2 .{ x: 10, y: 20 };
	vec2_print(vec);
	vec2_print(vec2_add(vec, .{ x: 5, y: 5 }));
	0
}

fn printf(fmt: *i8, ...);
