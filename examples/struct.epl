struct Rect {
	x: i32,
	y: i32,
	width: i32,
	height: i32,
}

fn rect_move(rect: Rect, dx: i32, dy: i32) -> Rect {
	rect.x += dx;
	rect.y += dy;
	rect
}

fn rect_print(rect: Rect) -> void {
	printf("Rect { %i %i %i %i }\n", rect.x, rect.y, rect.width, rect.height);
}

fn main() -> i32 {
	let rect = Rect {
		x: 10,
		y: 20,
		width: 5,
		height: 5,
	};
	rect_print(rect);
	rect_print(rect_move(rect, 5, 5));
	0
}

fn printf(fmt: cstr, ...) -> void;
