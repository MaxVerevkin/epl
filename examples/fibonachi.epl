fn fib(n: u32) -> u32 {
	if n < 2 {
		n
	} else {
		fib(n - 2) + fib(n - 1)
	}
}

fn fib_it(n: u32) -> u32 {
	let a: u32 = 0;
	let b: u32 = 1;
	let i: u32 = 0;
	while i < n {
		let tmp = a;
		a = b;
		b += tmp;
		i += 1;
	}
	a
}

fn main() -> i32 {
	let n: u32 = 0;
	while n < 10 {
		printf("fib(%i) = %i\n", n, fib(n));
		printf("fib_id(%i) = %i\n", n, fib_it(n));
		n += 1;
	}
	0
}

